# mount     fstype  device                 [device2]        [fstype2]   [fsoptions]   [fsoptions2]
/boot       bml     boot
/recovery   bml     recovery
/system     ext4    /dev/block/stl17       /dev/block/stl16 rfs         NULL          rw,check=no
/data       ext4    /dev/block/stl16       /dev/block/stl17 rfs         NULL          rw,nosuid,nodev,check=no
/cache      ext4    /dev/block/stl20       NULL             rfs         NULL          rw,nosuid,nodev,check=no
/sdcard     vfat    /dev/block/mmcblk0p1 
/sd-ext     ext4    /dev/block/mmcblk0p2   NULL             NULL        NULL          rw,nosuid,nodev,check=no
